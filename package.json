{
  "name": "react-redux-template",
  "version": "1.0.0",
  "description": "starter project for a web app with react & redux",
  "main": "app/index.js",
  "scripts": {
    "test": "NODE_ENV=test NODE_PATH=./app mocha --compilers js:babel-register -r app/spec/setup.mocha.js --recursive 'app/spec/*.spec.js' --reporter spec",
    "tdd": "NODE_ENV=test NODE_PATH=./app mocha --compilers js:babel-register -r app/spec/setup.mocha.js --recursive 'app/spec/*.spec.js' -w",
    "start": "NODE_ENV=development node_modules/.bin/webpack-dev-server --colors --port 8080 --inline",
    "dev": "npm run start",
    "build": "NODE_ENV=production node_modules/.bin/webpack --optimize-minimize --progress --colors --display-error-details",
    "build-staging": "NODE_ENV=staging node_modules/.bin/webpack --progress --colors --devtool #source-map"
  },
  "repository": {
    "type": "git"
  },
  "license": "MIT",
  "dependencies": {
    "axios": "0.13.1",
    "babel-polyfill": "^6.8.0",
    "classnames": "^2.2.5",
    "immutable": "^3.8.1",
    "moment": "2.14.1",
    "query-string": "4.2.2",
    "react": "^15.0.2",
    "react-dom": "^15.0.2",
    "react-redux": "^4.4.5",
    "react-router": "^2.4.0",
    "react-router-redux": "^4.0.4",
    "rebass": "^0.2.6",
    "redux": "^3.5.2",
    "redux-devtools": "^3.2.0",
    "redux-logger": "^2.6.1",
    "redux-thunk": "^2.0.1",
    "rx": "^4.1.0",
    "uuid": "^2.0.2"
  },
  "devDependencies": {
    "babel": "^6.5.2",
    "babel-eslint": "^6.0.4",
    "babel-loader": "^6.2.4",
    "babel-plugin-rewire": "1.0.0-rc-6",
    "babel-preset-es2015": "^6.6.0",
    "babel-preset-react": "^6.5.0",
    "babel-preset-stage-0": "^6.5.0",
    "babel-register": "^6.8.0",
    "chai": "^3.5.0",
    "chai-as-promised": "^5.3.0",
    "clean-webpack-plugin": "^0.1.9",
    "css-loader": "^0.23.1",
    "eslint": "^2.9.0",
    "eslint-plugin-babel": "^3.2.0",
    "eslint-plugin-react": "^5.1.1",
    "file-loader": "^0.8.5",
    "html-webpack-plugin": "^2.16.2",
    "jsdom": "^8.5.0",
    "json-loader": "^0.5.4",
    "karma": "^0.13.22",
    "mocha": "^2.5.3",
    "node-sass": "^3.8.0",
    "react-hot-loader": "^1.3.0",
    "react-scroll": "1.2.0",
    "react-tap-event-plugin": "1.0.0",
    "sass-loader": "^4.0.0",
    "sinon": "^1.17.4",
    "sinon-chai": "^2.8.0",
    "style-loader": "^0.13.1",
    "url-loader": "^0.5.7",
    "webpack": "^1.12.1",
    "webpack-dev-server": "^1.14.1"
  }
}
